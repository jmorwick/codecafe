    <bean id="whatHourIsIt" class="net.sourcedestination.codecafe.structure.exercises.ExpressionExercise">
        <constructor-arg name="exerciseId" value="whatHourIsIt"/>
        <constructor-arg name="timeout" value="1000"/>
        <constructor-arg name="initializationCode">
            <list>
                <value>long currentTimeMillis() { return System.currentTimeMillis(); }</value>
            </list>
        </constructor-arg>
        <constructor-arg name="targetType" value="int"/>
        <constructor-arg name="description">
            <bean id="outside-map" class="net.sourcedestination.codecafe.structure.exercises.ExerciseDefinition" factory-method="loadTextFile">
                <constructor-arg value="exercise-descriptions/whatHourIsIt.html"/>
            </bean>
        </constructor-arg>
        <constructor-arg name="goals">
            <bean class="net.sourcedestination.codecafe.structure.goals.GoalStructure">
                <constructor-arg name="shortDescription" value=""/>
                <constructor-arg name="longDescription" value="" />
                <constructor-arg name="subgoals">
                    <list>
                        <bean class="net.sourcedestination.codecafe.structure.goals.ExpressionTest">
                            <constructor-arg name="expectedOutput" value="#{2}"/>
                            <constructor-arg name="initializationCode"
                                             value="long currentTimeMillis() { return 1565984600627L; }"/>
                            <constructor-arg name="hiddenTest" value="false"/>
                            <constructor-arg name="shortDescription"
                                             value="Check time at 2:43 on 8/16/2019"/>
                            <constructor-arg name="id" value="test1"/>
                        </bean>
                        <bean class="net.sourcedestination.codecafe.structure.goals.ExpressionTest">
                            <constructor-arg name="expectedOutput" value="#{6}"/>
                            <constructor-arg name="initializationCode"
                                             value="long currentTimeMillis() { return 1566948600000L; }"/>
                            <constructor-arg name="hiddenTest" value="false"/>
                            <constructor-arg name="shortDescription"
                                             value="Start of the first online class"/>
                            <constructor-arg name="id" value="test2"/>
                        </bean>
                        <bean class="net.sourcedestination.codecafe.structure.goals.ExpressionTest">
                            <constructor-arg name="expectedOutput" value="#{10}"/>
                            <constructor-arg name="initializationCode"
                                             value="long currentTimeMillis() { return 1566834600000L; }"/>
                            <constructor-arg name="hiddenTest" value="false"/>
                            <constructor-arg name="shortDescription"
                                             value="End of the first live class"/>
                            <constructor-arg name="id" value="test3"/>
                        </bean>
                        <bean class="net.sourcedestination.codecafe.structure.goals.ExpressionTest">
                            <constructor-arg name="expectedOutput" value="#{10}"/>
                            <constructor-arg name="initializationCode"
                                             value="long currentTimeMillis() { return 1566834600000L; }"/>
                            <constructor-arg name="hiddenTest" value="true"/>
                            <constructor-arg name="shortDescription"
                                             value="Check time at 2:00 on 8/16/2019"/>
                            <constructor-arg name="id" value="test4"/>
                        </bean>
                        <bean class="net.sourcedestination.codecafe.structure.goals.ExpressionTest">
                            <constructor-arg name="expectedOutput" value="#{7}"/>
                            <constructor-arg name="initializationCode"
                                             value="long currentTimeMillis() { return 0L; }"/>
                            <constructor-arg name="hiddenTest" value="true"/>
                            <constructor-arg name="shortDescription"
                                             value="Time at the UNIX epoch"/>
                            <constructor-arg name="id" value="test5"/>
                        </bean>
                    </list>
                </constructor-arg>
            </bean>
        </constructor-arg>
    </bean>

    <bean id="gaussianPDFExpression" class="net.sourcedestination.codecafe.structure.exercises.MethodlikeExpressionExercise">
        <constructor-arg name="exerciseId" value="gaussianPDFExpression"/>
        <constructor-arg name="timeout" value="1000"/>
        <constructor-arg name="parameters">
            <map>
                <entry key="µ" value="#{1.0}"/>
                <entry key="σ" value="#{2.0}"/>
                <entry key="x" value="#{3.0}"/>
            </map>
        </constructor-arg>
        <constructor-arg name="targetType" value="double"/>
        <constructor-arg name="description">
            <bean id="outside-map" class="net.sourcedestination.codecafe.structure.exercises.ExerciseDefinition" factory-method="loadTextFile">
